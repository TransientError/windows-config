{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/refs/heads/master/schema.json",
  "minimum_window_width": 0,
  "minimum_window_height": 0,
  "resize_delta": 50,
  "window_container_behaviour": "Create",
  "cross_monitor_move_behaviour": "Insert",
  "cross_boundary_behaviour": "Workspace",
  "unmanaged_window_operation_behaviour": "Op",
  "mouse_follows_focus": true,
  "border_width": 8,
  "border_offset": -1,
  "border": true,
  "border_colours": {
    "single": {
      "r": 142,
      "g": 140,
      "b": 216
    },
    "stack": {
      "r": 0,
      "g": 165,
      "b": 66
    },
    "monocle": {
      "r": 255,
      "g": 51,
      "b": 153
    },
    "unfocused": {
      "r": 128,
      "g": 128,
      "b": 128
    }
  },
  "border_style": "System",
  "border_z_order": "Bottom",
  "border_implementation": "Komorebi",
  "transparency": true,
  "transparency_alpha": 200,
  "default_workspace_padding": 10,
  "default_container_padding": 10,
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "Columns",
          "layout_rules": {},
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "II"
        }
      ],
      "window_based_work_area_offset_limit": 1
    }
  ],
  "window_hiding_behaviour": "Minimize",
  "monitor_index_preferences": {},
  "display_index_preferences": {},
  "slow_application_compensation_time": 50,
  "slow_application_identifiers": [
    {
      "id": "zen.exe",
      "kind": "Exe",
      "matching_strategy": "Equals"
    }
  ],
  "app_specific_configuration_path": "C:/Users/kvwu/applications.json",
}
